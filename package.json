{
    "name": "vscode-quickopen",
    "displayName": "vscode-quickopen",
    "description": "",
    "publisher": "vince-fugnitto",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.46.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "vscode-quickopen.prefix",
                "title": "QuickOpen: Open (Prefix)"
            },
            {
                "command": "vscode-quickopen.no-prefix",
                "title": "QuickOpen: Open (No Prefix)"
            },
            {
                "command": "vscode-quickopen.invalid-prefix",
                "title": "QuickOpen: Open (Invalid Prefix)"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.46.0",
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.4",
        "@types/node": "^12.11.7",
        "eslint": "^7.15.0",
        "@typescript-eslint/eslint-plugin": "^4.9.0",
        "@typescript-eslint/parser": "^4.9.0",
        "glob": "^7.1.6",
        "mocha": "^8.1.3",
        "typescript": "^4.1.2",
        "vscode-test": "^1.4.1"
    }
}
